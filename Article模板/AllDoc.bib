@Article{Baxter2006MandarinDialectPhylogeny,
  author       = {William H. Baxter},
  date         = {2006},
  journaltitle = {Cahiers de linguistique - Asie orientale},
  title        = {Mandarin dialect phylogeny},
  abstract     = {nullCet article analyse les relations phylogénétiques entre dix dialectes chinois, dont huit dits 'mandarins' et deux autres (Changshâ jkfà et Hangzhôu trC jf[). Sur la base de six des dialectes mandarins, un système phonologique 'proto-macro-mandarin' (PMM) est reconstruit; il semble aussi possible d'en dériver les quatre autres dialectes. Sur la base de 29 innovations phonologiques, la phylogénie des dix dialectes est estimée avec une technique empruntée à la biologie (Camin-Sokal parsimony). Les résultats indiquent qu'une classification phylogénétique des dialectes chinois pourrait être bien différente de la classification généralement acceptée. 

This paper investigates the phylogenetic relationships (the relationships of shared ancestry) of ten Chinese dialects: eight generally classified as Mandarin, and two others (Changshâ Лй> and Hangzhou Ш1|). Using six of the Mandarin dialects, a 'Proto-Macro-Mandarin' (PMM) phonological system is reconstructed, from which the other four dialects also appear to be derivable. Taking 29 phonological innovations as a basis, the phylogeny of the ten dialects is estimated using a technique borrowed from biology (Camin-Sokal parsimony). The results suggest that a classification of Chinese dialects based on phylogeny could be rather different from the generally accepted classification.},
  file         = {:C\:/Users/11510/Desktop/Phylolinguistics/Mandarin dialect phylogeny.pdf:PDF},
}

